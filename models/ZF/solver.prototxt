train_net: "/home/ubuntu/dstl/models/ZF/train.prototxt"
test_net: "/home/ubuntu/dstl/models/ZF/test.prototxt"

test_iter: 100
test_interval: 200

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 50000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005

# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "zf_faster_rcnn"
iter_size: 2
